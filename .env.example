# Environment
ENV=development

# Database
DATABASE_URL=postgres://postgres:postgres@localhost:5432/your_db_name?sslmode=disable

# Server
PORT=8080

# Logging
LOG_LEVEL=debug 

# JWT
ACCESS_TOKEN_SECRET=your-256-bit-secret
REFRESH_TOKEN_SECRET=your-256-bit-refresh-secret
ACCESS_TOKEN_EXPIRY=15    # 15 minutes
REFRESH_TOKEN_EXPIRY=168  # 7 days

# Rate Limiting
RATE_LIMIT_REQUESTS=100   # requests
RATE_LIMIT_DURATION=1     # minute